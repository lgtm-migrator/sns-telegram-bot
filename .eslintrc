{
    "extends": "webbylab",
    "env": {
        "browser": false
    },
    "rules": {
        "import/no-commonjs": 0,
        "func-names": 0,
        "func-style": 0,
        "prefer-arrow-callback": 0,
        "linebreak-style": 0,
        "more/no-numeric-endings-for-variables": 0,
        "no-unused-expressions": 0
    },
    "settings": {
        "import/resolver": {
          "babel-module": {}
        }
    }
}